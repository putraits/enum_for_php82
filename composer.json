{
    "name": "putraits/enums",
    "description": "ProjectICKX PHP utility traits for enum",
    "type": "library",
    "homepage": "https://ickx.jp",
    "license": "MIT",
    "authors": [
        {
            "name": "akira wakaba",
            "homepage": "https://ickx.jp",
            "role": "lead developer"
        }
    ],
    "config": {
        "autoloader-suffix": "putraits_enums"
    },
    "autoload": {
        "psr-4": {
            "putraits\\enums\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "putraits\\enums\\tests\\": "./tests/"
        }
    },
    "keywords": [
        "enum"
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~10.0.0",
        "friendsofphp/php-cs-fixer": "~3.21.0"
    }
}